#!/bin/bash

echo "running 'reset'"

rm -rf bitdust
git clone https://github.com/vesellov/bitdust.devel.git
# git clone http://gitlab.bitdust.io/devel/bitdust.devel.git
mv bitdust.devel bitdust
cd bitdust
git config --global user.email "vesellov@gmail.com"
git config --global user.name "Veselin VPS"
git status
cd ..

